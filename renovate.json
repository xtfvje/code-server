{
  "rangeStrategy": "replace",
  "extends": ["config:base"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "digest"],
      "automerge": true,
      "groupName": "Minor dependency updates"
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": "true"
  },
  "ignoreDeps": ["express"]
}
